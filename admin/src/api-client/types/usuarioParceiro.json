{
  "type": "object",
  "properties": {
    "id": { "type": "number", "description": "ID único da relação usuário-parceiro", "example": 1 },
    "usuarioId": { "type": "number", "description": "ID do usuário", "example": 1 },
    "parceiroId": { "type": "number", "description": "ID do parceiro", "example": 1 },
    "perfilId": { "type": "number", "description": "ID do perfil", "example": 1 },
    "createdAt": { "format": "date-time", "type": "string", "description": "Data de criação da relação", "example": "2024-01-01T00:00:00Z" },
    "Usuario": {
      "description": "Dados do usuário relacionado",
      "allOf": [
        {
          "type": "object",
          "properties": {
            "id": { "type": "number", "description": "ID interno do usuário", "example": 1 },
            "publicId": { "type": "string", "description": "ID público do usuário (UUID v7)", "example": "01234567-89ab-cdef-0123-456789abcdef" },
            "nome": { "type": "string", "description": "Nome completo do usuário", "example": "João Silva" },
            "email": { "type": "string", "description": "Email do usuário", "example": "joao@exemplo.com" },
            "telefone": { "type": "string", "description": "Telefone do usuário para SMS ou WhatsApp", "example": "+5511999999999" },
            "provider": { "type": "string", "description": "Provedor de autenticação", "example": "LOCAL", "default": "LOCAL" },
            "googleId": { "type": "string", "description": "ID do Google para autenticação OAuth", "example": "google_123456789" },
            "senha": { "type": "string", "description": "Senha do usuário (hash)", "example": "$2b$10$..." },
            "ativo": { "type": "boolean", "description": "Status ativo do usuário", "example": true, "default": true },
            "avatar": { "type": "string", "description": "URL do avatar do usuário", "example": "https://exemplo.com/avatar.jpg" },
            "createdAt": { "format": "date-time", "type": "string", "description": "Data de criação do usuário", "example": "2024-01-01T00:00:00.000Z" }
          },
          "required": ["id", "publicId", "nome", "email", "provider", "ativo", "createdAt"],
          "x-readme-ref-name": "Usuario"
        }
      ]
    },
    "Parceiro": { "description": "Dados do parceiro relacionado", "allOf": [{ "$ref": "#/components/schemas/Parceiro" }] },
    "perfil": {
      "description": "Dados do perfil relacionado",
      "allOf": [
        {
          "type": "object",
          "properties": {
            "id": { "type": "number", "description": "ID interno do perfil", "example": 1 },
            "nome": { "type": "string", "description": "Nome do perfil", "example": "Administrador" },
            "ativo": { "type": "boolean", "description": "Status ativo do perfil", "example": true, "default": true }
          },
          "required": ["id", "nome", "ativo"],
          "x-readme-ref-name": "Perfil"
        }
      ]
    }
  },
  "required": ["id", "usuarioId", "parceiroId", "perfilId", "createdAt"],
  "x-readme-ref-name": "UsuarioParceiro"
}
